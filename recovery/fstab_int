# /etc/fstab: static file system information
#
# file system	mount point	type		options	dump pass
/dev/block/mtdblock1 /cache yaffs2 defaults 0 0
/dev/block/mtdblock5 /system yaffs2 defaults 0 0
/dev/block/mtdblock6 /data yaffs2 defaults 0 0
/dev/block/mmcblk0p1 /sdcard/ vfat defaults 0 0
/dev/block/mmcblk0p2 /sd-ext ext4 defaults 0 0
/dev/block/mmcblk0p3 /swap swap defaults 0 0
